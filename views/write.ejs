      <!-- Main Card -->

      <div class="row">
        <form action="/submitJournal" method="POST">
          <div class="main-card col-8 px-4 py-3 mb-5">
            <div class="mt-4 mb-3">
              <div class="row">
                <div class="col-10">
                  <label for="title" class="form-label">Title</label>
                </div>
                <div class="col-2">
                  <p class="journal-date"><%= currentTime %></p>
                </div>
              </div>

              <div class="col-4">
                <input
                  type="text"
                  name="newJournalTitle"
                  class="title-input form-control"
                  id="title"
                  placeholder="Your Title..."
                />
              </div>
            </div>
            <div class="mb-3">
              <label for="entry" class="form-label">Your Journal</label>
                <textarea
                class="entry-textarea form-control"
                id="entry"
                name="newJournalEntry"
                rows="12"
                placeholder="Journal Entry..."
              ></textarea>

            </div>
            <button type="submit" class="btn custom-btn-color">Save Entry</button>

          </div>
        </form>
      </div>
    </div>
    <footer>
        <div class="container">
          <div class="row">
            <div class="col-md-12 d-flex justify-content-center">
              <p>&copy; 2023 LifeLog.</p>
            </div>
          </div>
        </div>
      </footer>

      <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

      <script>
document.querySelector("form").addEventListener('submit', function(event) {
    event.preventDefault();

    const data = {
        newJournalTitle: document.querySelector("input[name='newJournalTitle']").value,
        newJournalEntry: document.querySelector("textarea[name='newJournalEntry']").value
    };
    
    fetch('/submitJournal', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(data)
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            Swal.fire(
              'Success',
              'Your journal has been saved!',
              'success'
            ).then(() => {
                window.location.reload();
            });
        } else {
            Swal.fire(
              'Error',
              'There was an issue saving your journal.',
              'error'
            );
        }
    })
    .catch(error => {
        Swal.fire(
          'Error',
          'There was an error processing your request.',
          'error'
        );
    });
});




      </script>
